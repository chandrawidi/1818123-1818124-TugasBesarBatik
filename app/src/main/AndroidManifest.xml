<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.Batik_Tubes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/birdworld"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.app.Batik_Tubes.UpdateActivity"></activity>
        <activity android:name="com.app.Batik_Tubes.AboutActivity" />
        <activity android:name="com.app.Batik_Tubes.UpdelActivty" />
        <activity android:name="com.app.Batik_Tubes.CreateActivity" />
        <activity android:name="com.app.Batik_Tubes.ReadActivity" />
        <activity
            android:name="com.app.Batik_Tubes.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>